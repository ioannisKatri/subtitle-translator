{
  "name": "upload-api",
  "version": "1.0.0",
  "description": "Receives files through a REST endpoint,  uploads them on S3 and notify the Amazon SQS",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon app/index.ts",
    "test": "jest app/tests --testEnvironment=node && export NODE_ENV=test",
    "start": "node build && export NODE_ENV=production",
    "build": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express-winston": "^4.0.0",
    "@types/datadog-winston": "^1.0.4",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.22",
    "@types/nodemon": "^1.19.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.2.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "datadog-winston": "^1.5.1",
    "express-winston": "^4.0.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 5000,
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
